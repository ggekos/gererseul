{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
    {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}

    {% for message in app.flashes('success') %}
        <div class="flash-notice">{{ message }}</div>
    {% endfor %}
{% endblock %}
